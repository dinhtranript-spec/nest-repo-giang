{
  "name": "@aisoft/backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "aisoft": "lib/@aisoft/cli/bin/aisoft.js"
  },
  "scripts": {
    "prepare": "husky",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build-bundle": "npm run prebuild && nest build && javascript-obfuscator ./dist --output ./dist-bundle && cp -r dist-bundle/* dist && rm -rf dist-bundle",
    "format": "prettier --cache --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "aisoft": "npm link"
  },
